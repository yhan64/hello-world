{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    // "lib": ["ES2019"],
    // "declaration": true,
    "rootDirs": ["src", "test"],
    "outDir": "dist",
    "esModuleInterop": true,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "test"]
}
